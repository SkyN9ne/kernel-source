 	for (j = 0; j < templates; j++, fwdt++) {
-		if (fwdt->template)
-			vfree(fwdt->template);
+		vfree(fwdt->template);
 		fwdt->template = NULL;
 		fwdt->length = 0;
 
@@ -8196,8 +8192,7 @@ qla24xx_load_risc_blob(scsi_qla_host_t *
 	return QLA_SUCCESS;
 
 failed:
