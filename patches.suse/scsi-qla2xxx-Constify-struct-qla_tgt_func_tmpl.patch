 	struct qla_tgt_vp_map *tgt_vp_map;
 
 	int saved_set;
--- a/drivers/scsi/qla2xxx/tcm_qla2xxx.c
+++ b/drivers/scsi/qla2xxx/tcm_qla2xxx.c
@@ -1631,7 +1631,7 @@ static void tcm_qla2xxx_update_sess(stru
 /*
